{
  "name": "pushprinter",
  "version": "2.0.0",
  "description": "Automatically print files from servers",
  "main": "gui/dist/index.js",
  "bin": "headless/src/app.js",
  "scripts": {
    "test": "nodemon --ignore tmp/ --exec babel-node ./service/test",
    "dist-headless-service": "webpack --config ./headless/src/webpack.config.js --progress",
    "dist-headless-service-m": "babel ./headless/src/service.js --out-file ./headless/src/assets/service.js --minified --compact",
    "dist-headless-app": "pkg . --output ./headless/dist/app.exe --targets win",
    "dist-headless": "npm run dist-headless-service && npm run dist-headless-app",
    "gui-clean": "shx rm -rf gui/dist/",
    "gui-test": "npm run gui-clean && concurrently -k \"npm run gui-test-main\" \"npm run gui-test-render\"",
    "gui-test-main": "rollup -c gui/rollup.js --watch",
    "gui-test-render": "webpack -d --config gui/webpack.dev.js",
    "gui-test-start": "cross-env NODE_ENV=development electron .",
    "gui-dist-main": "rollup -c gui/rollup.js",
    "gui-dist-render": "webpack -p --config gui/webpack.js",
    "gui-dist-build": "electron-builder -w --ia32",
    "gui-dist": "npm run gui-clean && npm run gui-dist-main && npm run gui-dist-render && npm run gui-dist-build",
    "postinstall": "electron-builder install-app-deps"
  },
  "pkg": {
    "assets": "headless/src/assets/service.js"
  },
  "build": {
    "nodeGypRebuild": false,
    "productName": "PushPrinter",
    "appId": "com.electron.autoPrinter.utility",
    "copyright": "Copyright Â© 2018 CloudWaitress",
    "directories": {
      "buildResources": "gui/resources",
      "output": "gui/builds"
    },
    "win": {
      "target": "nsis",
      "icon": "gui/resources/icon.ico"
    }
  },
  "author": "CloudWaitress <admin@cloudwaitress.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "electron": "^2.0.1",
    "electron-builder": "^20.13.4",
    "electron-packager": "^12.1.0",
    "electron-rebuild": "^1.7.3",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "postcss-loader": "^2.1.5",
    "resolve-url-loader": "^2.3.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sass-loader": "^7.0.1",
    "shx": "^0.2.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "ably": "^1.0.13",
    "animate.css": "^3.6.1",
    "async-exit-hook": "^2.0.1",
    "axios": "^0.18.0",
    "cacheman-file": "^0.2.1",
    "class-autobind": "^0.1.4",
    "classnames": "^2.2.5",
    "compare-versions": "^3.2.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^0.3.0",
    "electron-json-storage": "^4.1.0",
    "flat-cache": "^1.3.0",
    "g-notifications": "^0.3.0",
    "noty": "^3.2.0-beta",
    "pm2": "^2.10.3",
    "raven": "^2.6.2",
    "raven-js": "^3.25.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-onclickout": "^2.0.8",
    "react-redux": "^5.0.7",
    "react-spinners": "^0.3.2",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "shelljs": "^0.8.2",
    "shortid": "^2.2.8",
    "tail": "^1.2.3",
    "velocity-animate": "^1.5.1",
    "yargs": "^11.0.0"
  }
}
